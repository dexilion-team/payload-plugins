#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mtolmacs/Projects/dexilion-team/dexilion.com/node_modules/.pnpm/vitest@4.0.16_@types+node@25.0.3_sass@1.77.4_tsx@4.20.6/node_modules/vitest/node_modules:/Users/mtolmacs/Projects/dexilion-team/dexilion.com/node_modules/.pnpm/vitest@4.0.16_@types+node@25.0.3_sass@1.77.4_tsx@4.20.6/node_modules:/Users/mtolmacs/Projects/dexilion-team/dexilion.com/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mtolmacs/Projects/dexilion-team/dexilion.com/node_modules/.pnpm/vitest@4.0.16_@types+node@25.0.3_sass@1.77.4_tsx@4.20.6/node_modules/vitest/node_modules:/Users/mtolmacs/Projects/dexilion-team/dexilion.com/node_modules/.pnpm/vitest@4.0.16_@types+node@25.0.3_sass@1.77.4_tsx@4.20.6/node_modules:/Users/mtolmacs/Projects/dexilion-team/dexilion.com/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
