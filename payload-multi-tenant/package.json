{
  "name": "@dexilion/payload-multi-tenant",
  "version": "3.69.0",
  "description": "A Payload CMS plugin to enable multi-tenant functionality.",
  "author": "Dexilion",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./TenantSelect": {
      "types": "./src/components/TenantSelect.tsx",
      "import": "./src/components/TenantSelect.tsx",
      "default": "./src/components/TenantSelect.tsx"
    },
    "./GlobalViewRedirect": {
      "types": "./src/components/GlobalViewRedirect.tsx",
      "import": "./src/components/GlobalViewRedirect.tsx",
      "default": "./src/components/GlobalViewRedirect.tsx"
    },
    "./GlobalCollectionRedirect": {
      "types": "./src/components/GlobalCollectionRedirect.tsx",
      "import": "./src/components/GlobalCollectionRedirect.tsx",
      "default": "./src/components/GlobalCollectionRedirect.tsx"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "next": "^15.4",
    "payload": "3",
    "@payloadcms/ui": "3",
    "@dexilion/payload-utils": "3.69.0"
  },
  "devDependencies": {
    "@payloadcms/db-mongodb": "3.69.0",
    "@types/react": "19.2.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "mongodb-memory-server": "^10.2.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest"
  }
}
