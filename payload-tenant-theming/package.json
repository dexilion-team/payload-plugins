{
  "name": "@dexilion/payload-tenant-theming",
  "version": "3.69.0",
  "description": "A Payload CMS plugin to provide theming capabilities for multi-tenant setups.",
  "author": "Dexilion",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./Page": {
      "types": "./src/components/Page.tsx",
      "import": "./src/components/Page.tsx",
      "default": "./src/components/Page.tsx"
    },
    "./Layout": {
      "types": "./src/components/Layout.tsx",
      "import": "./src/components/Layout.tsx",
      "default": "./src/components/Layout.tsx"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "dependencies": {
    "@dexilion/payload-multi-tenant": "workspace:*",
    "@dexilion/payload-nested-docs": "workspace:*"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "next": "^15.4",
    "payload": "3",
    "@payloadcms/ui": "3"
  },
  "devDependencies": {
    "@types/react": "19.2.2",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "typescript": "^5.9.3"
  }
}
